    <navigation-rule id="2">
        <description>Direccionamiento</description>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>listarClasificacion</from-outcome>
            <to-view-id>/pages/clasificacion/listarClasificacion.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
